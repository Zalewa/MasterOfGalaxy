{
  i18n: {
    bundle: "i18n/planets"
  },
  planets: {
    none: {
      typeName: "$noPlanet",
      textureName: "sprite/star.png",
      starChance: {
        default: 0.1
      }
    },
    terran: {
      typeName: "$terran",
      textureName: "",
      starChance: {
        default: 0.2,
        stars: {
          yellow: 1.0,
          green: 0.8,
          red: 0.3,
        }
      }
    },
    desert: {
      typeName: "$desert",
      textureName: "",
      starChance: {
        default: 0.4,
        stars: {
          yellow: 0.5,
          red: 0.5,
          white: 0.5,
          blue: 0.5
        }
      }
    },
    jungle: {
      typeName: "$jungle",
      textureName: "",
      starChance: {
        default: 0.1,
        stars: {
          yellow: 0.8,
          green: 0.7,
          red: 0.4,
        }
      }
    },
    water: {
      typeName: "$water",
      textureName: "",
      starChance: {
        default: 0.1,
        stars: {
          yellow: 0.9,
